@page
@model WebApplication22.Pages.Admin.Credito.IndexModel
@{ Layout = null; }
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        <a asp-page="Create">Criar Novo Registro</a>
    </p>
    <table>
        <tr>
            <th>Id</th>
            <th>Descrição</th>
            <th>...</th>
        </tr>
        @foreach (var item in Model.Credits)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Description</td>
                <td>
                    <a asp-page="Edit" asp-route-id="@item.Id">Alterar Registro</a> |
                    <a asp-page="Details" asp-route-id="@item.Id">Detalhes</a> |
                    <a asp-page="Delete" asp-route-id="@item.Id">Excluir</a>
                </td>
            </tr>
        }
    </table>
</body>
</html>